#include "artdaq/BuildInfo/GetPackageInfo.h"

#include <string>

namespace artdaq {

  artdaq::PackageBuildInfo PackageInfo::getPackageBuildInfo() {

    artdaq::PackageBuildInfo pkg;

    pkg.setPackageName("artdaq");
    pkg.setPackageVersion("@version@");
    pkg.setBuildTimestamp("@utcDateTime@");

    return pkg;
  }

}

